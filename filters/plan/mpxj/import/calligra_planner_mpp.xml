<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/vnd.ms-project">
    <comment>MS Project plan</comment>
    <magic priority="50">
      <match type="string" value="\xd0\xcf\x11\xe0\xa1\xb1\x1a\xe1\x00" offset="0"/>
    </magic>
    <glob pattern="*.mpp"/>
    <generic-icon name="application-x-vnd.kde.plan"/>
  </mime-type>
  <mime-type type="application/x-planner">
    <sub-class-of type="application/xml"/>
    <comment>Planner project plan</comment>
    <magic priority="60">
      <match type="string" value="mrproject-version" offset="20:140"/>
    </magic>
    <glob pattern="*.planner"/>
    <glob pattern="*.mrproject"/>
    <generic-icon name="x-vnd.kde.plan"/>
  </mime-type>
</mime-info>
