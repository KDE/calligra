msgstr "O SIGN(15) é igual a 1"

#: functions/math.xml:267
#, kde-format
msgid "SIGN(0) equals 0"
msgstr "O SIGN(0) é igual a 0"

#: functions/math.xml:268
#, kde-format
msgid "SIGN(-5) equals -1"
msgstr "O SIGN(5) é igual a -1"

