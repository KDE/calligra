msgstr "WEEKSINYEAR(рік)"

#: functions/datetime.xml:453
#, kde-format
msgid "WEEKSINYEAR(2000) returns 52"
msgstr "WEEKSINYEAR(2000) повертає 52"

#: functions/datetime.xml:465
#, kde-format
