msgstr "NETWORKDAY(Початкова дата; Кінцева дата; Вихідні)"

#: functions/datetime.xml:707
#, kde-format
msgid "NETWORKDAY(\"01/01/2001\";\"01/08/2001\") returns 5 workdays"
msgstr ""
"NETWORKDAY(\"01.01.2001\";\"08.08.2001\") повертає значення у 5 робочих днів"

#: functions/datetime.xml:708
#, kde-format
msgid "NETWORKDAY(\"01/01/2001\";\"01/08/2001\";2) returns 3 workdays"
msgstr ""
"NETWORKDAY(\"01.01.2001\";\"08.01.2001\";2) повертає значення у 3 робочі дні"

