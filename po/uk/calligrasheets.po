#, kde-format
msgid "YEARFRAC(start date; end date; basis)"
msgstr "YEARFRAC(Початкова дата; Кінцева дата; Базис)"

#: functions/datetime.xml:669 functions/datetime.xml:692
#, kde-format
msgid "Start date"
msgstr "Початкова дата"

#: functions/datetime.xml:673
#, kde-format
msgid "Working days"
msgstr "Робочі дні"

