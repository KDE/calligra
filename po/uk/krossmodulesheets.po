msgid "Invalid symbol"
msgstr "Некоректний символ"

#: scripts/yfinance.py:69
msgid "Invalid date"
msgstr "Некоректна дата"

#: scripts/yfinance.py:81
msgid "Invalid type"
msgstr "Некоректний тип"

#: scripts/yfinance.py:99 scripts/yweather.py:68
