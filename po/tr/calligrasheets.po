msgstr "SUBTOTAL(1; A1:A5), 23,8 döndürür"

#: functions/math.xml:23
#, kde-format
msgid "SUBTOTAL(4; A1:A5) returns 56"
msgstr "SUBTOTAL(4; A1:A5), 56 döndürür"

#: functions/math.xml:24
#, kde-format
msgid "SUBTOTAL(9; A1:A5) returns 119"
msgstr "SUBTOTAL(9; A1:A5), 119 döndürür"

#: functions/math.xml:25
#, kde-format
