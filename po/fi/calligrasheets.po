#, kde-format
msgid "COUNT(-5;\"KSpread\";2) returns 2"
msgstr "COUNT(-5;\"KSpread\";2) palauttaa 2"

#: functions/math.xml:299
#, kde-format
msgid "COUNT(5) returns 1"
msgstr "COUNT(5) palauttaa 1"

