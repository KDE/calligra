#, kde-format
msgid "FIND(\"i\";\"Calligra\") returns 5"
msgstr "FIND(\"i\";\"Calligra\") palauttaa 5"

#: functions/text.xml:307
#, kde-format
msgid "FIND(\"a\";\"Sheets in Calligra\";4) returns 12"
msgstr "FIND(\"a\";\"Calligran Sheets\";4) palauttaa 8"

