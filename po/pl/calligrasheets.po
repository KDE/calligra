msgstr "CHOOSE(1; \"pierwszy\"; \"drugi\") zwraca \"pierwszy\""

#: functions/reference.xml:92
#, kde-format
msgid "CHOOSE(2; 3; 2; 4) returns 2"
msgstr "CHOOSE(2; 3; 2; 4) zwraca 2"

#: functions/reference.xml:104
#, kde-format
