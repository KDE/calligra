msgstr "EXPONDIST(3;0.5;0) równa się 0.111565"

#: functions/statistical.xml:325
#, kde-format
msgid "EXPONDIST(3;0.5;1) equals 0.776870"
msgstr "EXPONDIST(3;0.5;1) równa się 0.776870"

