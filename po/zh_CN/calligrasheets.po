msgstr "SEARCH(\"e\";\"Statements\";6) 返回 7"

#: functions/text.xml:103
#, kde-format
msgid "SEARCH(\"margin\";\"Profit Margin\") returns 8"
msgstr "SEARCH(\"margin\";\"Profit Margin\") 返回 8"

