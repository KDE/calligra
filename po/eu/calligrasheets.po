msgstr "LOG() funtzioak x-ren logaritmo hamartarra itzultzen du."

#: functions/math.xml:764
#, kde-format
msgid "LOG(x)"
msgstr "LOG(x)"

#: functions/math.xml:765
#, kde-format
msgid "LOG(0.8) equals -0.09691001"
msgstr "LOG(0,8) funtzioak -0,09691001 itzultzen du"

#: functions/math.xml:766
#, kde-format
msgid "LOG(0) is an error."
msgstr "LOG(0) funtzioak errore bat itzultzen du"

#: functions/math.xml:778
