#, kde-format
msgid "Converts text string that represents a value to the real value.  "
msgstr "Balio bat ordezkatzen duen testu-katea benetako balio bihurtzen du."

#: functions/text.xml:718
#, kde-format
msgid "VALUE(text)"
msgstr "VALUE(testua)"

#: functions/text.xml:719
#, kde-format
msgid "VALUE(\"14.03\") returns 14.03"
msgstr "VALUE(\"14,03\") funtzioak 14,03 itzultzen du"

#: functions/text.xml:727 functions/text.xml:756
#, kde-format
