msgstr "ROOTN() funtzioak x-ren n-garren erro ez negatiboa kalkulatzen du."

#: functions/math.xml:734
#, kde-format
msgid "ROOTN(x;n)"
msgstr "ROOTN(x;n)"

#: functions/math.xml:735
#, kde-format
msgid "ROOTN(9;2) equals 3"
msgstr "ROOTN(9;2) funtzioak 3 itzultzen du"

#: functions/math.xml:748
#, kde-format
