msgstr "LOGn() funtzioak x-ren n oinarriko logaritmoa itzultzen du."

#: functions/math.xml:711
#, kde-format
msgid "LOGn(value;base)"
msgstr "LOGn(balioa;oinarria)"

#: functions/math.xml:712
#, kde-format
msgid "LOGn(12;10) equals 1.07918125"
msgstr "LOGn(12;10) funtzioak 1,07918125 itzultzen du"

#: functions/math.xml:713
#, kde-format
msgid "LOGn(12;2) equals 3.5849625"
msgstr "LOGn(12;2) funtzioak 3,5849625 itzultzen du"

#: functions/math.xml:733
#, kde-format
