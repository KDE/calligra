msgstr "CUR() funtzioak x-ren erro kubikoa ez negatiboa itzultzen du."

#: functions/math.xml:749
#, kde-format
msgid "CUR(x)"
msgstr "CUR(x)"

#: functions/math.xml:750
#, kde-format
msgid "CUR(27) equals 3"
msgstr "CUR(27) funtzioak 3 itzultzen du"

#: functions/math.xml:759
#, kde-format
