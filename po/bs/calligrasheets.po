#, kde-format
msgid "LEVEL_COUPON(face value;coupon rate;coupons per year;years;market rate)"
msgstr ""
"LEVEL_COUPON(nominalna vrijednost;kuponska rata;kupon za godinu;godine;stopa "
"na tržištu)"

