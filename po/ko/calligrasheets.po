#: functions/datetime.xml:219
#, kde-format
msgid "SECOND(time)"
msgstr "SECOND(시간)"

#: functions/datetime.xml:220
#, kde-format
msgid "SECOND(\"22:10:12\") returns 12"
msgstr "SECOND(\"22:10:12\")는 12를 반환함"

#: functions/datetime.xml:221
#, kde-format
