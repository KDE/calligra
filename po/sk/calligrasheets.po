#, kde-format
msgid "SKEWP(range)"
msgstr "SKEWP(rozsah)"

#: functions/statistical.xml:1118
#, kde-format
msgid "SKEWP(11.4; 17.3; 21.3; 25.9; 40.1) returns 0.6552"
msgstr "SKEWP(11,4; 17,3; 21,3; 25,9; 40,1) vracia 0,6552"

#: functions/statistical.xml:1135
#, kde-format
