msgstr "STDEVPA(6; 7; A1; 8) es igual a 2.69..., si A1 es TRUE"

#: functions/statistical.xml:1303
#, kde-format
msgid "STDEVPA(6; 7; A1; 8) equals 3.11..., if A1 is FALSE"
msgstr "STDEVPA(6; 7; A1; 8) es igual a 3.11..., si A1 es FALSE"

#: functions/statistical.xml:1321
#, kde-format
