msgstr "GAMMADIST(GAMMAINV(0.1;3;4);3;4) es igual a 0.1"

#: functions/statistical.xml:474
#, kde-format
msgid "GAMMADIST(GAMMAINV(0.3;3;4);3;4) equals 0.3"
msgstr "GAMMADIST(GAMMAINV(0.3;3;4);3;4) es igual a 0.3"

