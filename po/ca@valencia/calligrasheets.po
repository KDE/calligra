#, kde-format
msgid "N(value)"
msgstr "N(valor)"

#: functions/information.xml:46
#, kde-format
msgid "N(3.14) returns 3.14"
msgstr "N(3,14) retorna 3,14"

#: functions/information.xml:47
#, kde-format
