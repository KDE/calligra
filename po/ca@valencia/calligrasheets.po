#, kde-format
msgid "EURO(currency)"
msgstr "EURO(moneda)"

#: functions/financial.xml:523
#, kde-format
msgid "EURO(\"DEM\") equals 1.95583"
msgstr "EURO(\"DEM\") fa 1,95583"

