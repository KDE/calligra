msgstr "STDEVPA(6; 7; A1; 8) fa 0,816497…, si A1 està buida"

#: functions/statistical.xml:1302
#, kde-format
msgid "STDEVPA(6; 7; A1; 8) equals 2.69..., if A1 is TRUE"
msgstr "STDEVPA(6; 7; A1; 8) fa 2,69…, si A1 és CERT"

#: functions/statistical.xml:1303
#, kde-format
