msgstr "STDEVA(6; 7; A1; 8) fa 1, si A1 està buida"

#: functions/statistical.xml:1238
#, kde-format
msgid "STDEVA(6; 7; A1; 8) equals 3.109, if A1 is TRUE"
msgstr "STDEVA(6; 7; A1; 8) fa 3,109, si A1 és CERT"

# skip-rule: punctuation-period-no
#: functions/statistical.xml:1268
#, kde-format
