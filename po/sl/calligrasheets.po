#, kde-format
msgid "KURTP(A1:A5)"
msgstr "KURTP(A1:A5)"

#: functions/statistical.xml:659
#, kde-format
msgid "KURTP(21; 33; 54; 23) returns -1.021"
msgstr "KURTP(21; 33; 54; 23) vrne -1,021"

