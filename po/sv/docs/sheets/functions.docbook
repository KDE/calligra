<varlistentry
><term
>Syntax</term>
<listitem
><para
>REPT(text;antal)</para
></listitem>
</varlistentry>
<varlistentry
><term
>Parametrar</term>
<listitem
><para
><segmentedlist
><segtitle
>Kommentar</segtitle
><segtitle
>Typ</segtitle>
<seglistitem
><seg
>Källsträng</seg
><seg
>Text</seg
></seglistitem>
<seglistitem
><seg
>Antal repetitioner</seg
><seg
>Heltal (som 1, 132 eller 2344)</seg
></seglistitem
></segmentedlist
></para
></listitem>
</varlistentry>
<varlistentry
><term
>Exempel</term>
<listitem
><para
>REPT("kspread";3) returnerar "kspreadkspreadkspread"</para
></listitem>
</varlistentry>
<varlistentry
><term
>Exempel</term>
<listitem
><para
>REPT("KSpread";0) returnerar ""</para
></listitem>
</varlistentry>
</variablelist>
</sect3>

